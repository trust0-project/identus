{
  "name": "@trust0/identus-react",
  "version": "0.0.135",
  "description": "React components for Hyperledger Identus, easily build your Identity applications on Cardano",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "typesVersions": {
    "*": {
      "*": "build/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./build",
    "./src"
  ],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js",
      "default": "./build/index.js"
    },
    "./context": {
      "types": "./build/context/index.d.ts",
      "import": "./build/context/index.mjs",
      "require": "./build/context/index.js",
      "default": "./build/context/index.js"
    },
    "./hooks": {
      "types": "./build/hooks/index.d.ts",
      "import": "./build/hooks/index.mjs",
      "require": "./build/hooks/index.js",
      "default": "./build/hooks/index.js"
    },
    "./resolver": {
      "types": "./build/resolver/index.d.ts",
      "import": "./build/resolver/index.mjs",
      "require": "./build/resolver/index.js",
      "default": "./build/resolver/index.js"
    },
    "./db": {
      "types": "./build/db/index.d.ts",
      "import": "./build/db/index.mjs",
      "require": "./build/db/index.js",
      "default": "./build/db/index.js"
    }
  },
  "scripts": {
    "build": "sh build.sh",
    "docs": "npx typedoc",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trust0-project/identus-react.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trust0-project/identus-react/issues"
  },
  "homepage": "https://github.com/trust0-project/identus-react#readme",
  "devDependencies": {
    "@esbuild-plugins/node-resolve": "^0.2.2",
    "@hyperledger/identus-sdk": "^7.0.0-rc.12",
    "@trust0/ridb-build": "^0.0.16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "nx": "20.8.1",
    "react": "^18",
    "react-dom": "^18",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@trust0/identus-store": "^1.0.21",
    "@trust0/ridb": "^1.5.31",
    "@trust0/ridb-core": "^1.7.26",
    "@trust0/ridb-react": "^1.4.10",
    "multiformats": "^9.9.0",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "@hyperledger/identus-sdk": "^7.0.0-rc.12",
    "react": "^18",
    "react-dom": "^18"
  }
}
